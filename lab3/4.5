import java.util.Scanner;
// e bine dar pe viitor sa incerci sa spargi in metode separate logica si doar un apel sa ai in main
public class Main {
    public static void main(String[] args) {
        //a
        long rez=1;
        int i;
        for(i=44;i<=49;i++)
            rez=rez*i/(49-i+1);
        System.out.println(rez);
        //b
        long loterie = 0;
        for(i = 1; i <= 6; i++){
            int bila = (int)(Math.random() * 49) + 1;
            long masca = 1L << bila;
            if((loterie & masca) == 0)
                loterie = loterie | masca;
            else
                i--;
        }

        // c
        for(i = 1; i <= 49; i++){
            if((1L << i & loterie) != 0)
                System.out.print(i + " ");
        }
    }

}
